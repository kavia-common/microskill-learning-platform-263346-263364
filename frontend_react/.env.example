# Frontend environment example
# PUBLIC_INTERFACE
# REACT frontend expects the backend base URL here.
# If empty, the app uses same-origin for API calls.
REACT_APP_API_BASE=http://localhost:3001

# Optional alternative backend url var (fallback if API_BASE missing)
REACT_APP_BACKEND_URL=

# Frontend URL for documentation/reference (not used by code directly)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional WebSocket (not used currently)
REACT_APP_WS_URL=

# Node environment flags
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1

# Logging
REACT_APP_LOG_LEVEL=info

# Feature flags (comma separated)
REACT_APP_FEATURE_FLAGS=

# Experiments
REACT_APP_EXPERIMENTS_ENABLED=false
