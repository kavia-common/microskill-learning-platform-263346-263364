# Frontend environment variables
# Set either REACT_APP_API_BASE or REACT_APP_BACKEND_URL to point to your backend.
# If neither is set, the app will call same-origin paths; in local dev it falls back to http://localhost:3001.
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
# Optional toggles
REACT_APP_EXPERIMENTS_ENABLED=false
REACT_APP_LOG_LEVEL=info
REACT_APP_AUDIO_DIAGNOSTIC=false
